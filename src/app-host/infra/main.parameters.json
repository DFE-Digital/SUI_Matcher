{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "environmentPrefix": {
        "value": "${AZURE_ENV_PREFIX}"
      },
      "containerAppManagedEnvironmentNumber": {
        "value": "${AZURE_CONTAINER_APP_MANAGED_ENVIRONMENT_NUMBER}"
      },
      "containerAppVnet": {
        "value": "${AZURE_CONTAINER_APP_VNET}"
      },
      "containerAppEnvSubnet": {
        "value": "${AZURE_CONTAINER_APP_ENV_SUBNET}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "monitoringActionGroupEmail": {
        "value": "${AZURE_MONITORING_ACTION_GROUP_EMAIL}"
      },
      "enableAuditLogging": {
        "value": "${AZURE_ENABLE_AUDIT_LOGGING}"
      }
    }
  }
  