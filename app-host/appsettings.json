{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Ingress": {
		"Port": 5000
	},
	"Aspire": {
		"Azure": {
		  "Security": {
			"KeyVault": {
			  "VaultUri": "Cooluri",
			  "DisableHealthChecks": false,
			  "DisableTracing": true,
			  "ClientOptions": {
				"DisableChallengeResourceVerification": true
			  }
			}
		  }
		}
	},
	"ConnectionStrings": {
		"secrets": "CoolConnection"
	}
}
